{
  "name": "Demo",
  "dark": true,
  "author": "Konstantin Bulenkov",

  "editorScheme": "/editorScheme.xml",

  "ui": {
    "*": {
      "background": "#000000"
    },
    "Label": {
      "foreground": "#aaaaaa"
    },
    "SidePanel.background": "#222222",
    "SearchMatch.startColor": "#FFD103",
    "SearchMatch.endColor": "#DDAF03",
    "SearchMatch.transparency": 70,

    "WelcomeScreen": {
      "background": "#000000",
      "Separator.color": "#000000",
      "Projects": {
        "background": "#080808",
        "selectionBackground": "#888888",
        "selectionInactiveBackground": "#444444"
      }
    }
  },

  "background": {
    "image": "/background.jpg",
    "transparency": 10,
    "fill": "scale",
    "anchor": "center"
  },

  "icons": {
    "/com/intellij/ide/ui/laf/icons/darcula/checkBox_dark.svg" : "/icons/checkBox.svg",
    "ColorPalette": {
      "Actions.Green": "#FF365D"
    }
  }
}